    overflow: hidden;
}
.img-hover-style img{
    margin: 0;
    transition: all 0.5s;
}
.img-hover-style:hover img{
    transform: scale(1.2);
}
.case-study-img-container {
    display: flex;
    gap: 20px;
    flex-wrap: wrap;
}
.case-study-img-container .card-img-container {
    width: calc((100% - 40px) / 3);
}
.card-img-container {
    position: relative;
    transition: all 0.3s;
}
.card-img-container::before {
    position: absolute;
    content: "";
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.1);
    z-index: 1;
}
.card-img-container::after {
    position: absolute;
    content: "";
    width: 100%;
    height: 100%;
    z-index: 1;
    left: 0;
    top: 0;
    transition: all 0.3s;
   
}
.card-img-container:hover::after {
    box-shadow: 0 0 0 25px rgba(255, 255, 255, 0.2) inset;
}
