  .hamBurgerIc svg {
    transition: transform 500ms cubic-bezier(0.4, 0, 0.2, 1);
  }
  .hamBurgerIc.activeM svg {
    transform: rotate(90deg);
  }
  .hamBurgerIc path {
    transition: transform 500ms cubic-bezier(0.4, 0, 0.2, 1),
      stroke-dasharray 500ms cubic-bezier(0.4, 0, 0.2, 1),
      stroke-dashoffset 500ms cubic-bezier(0.4, 0, 0.2, 1);
  }
  .hamBurgerIc path:nth-child(1) {
    transform-origin: 36% 40%;
  }
  .hamBurgerIc path:nth-child(2) {
    stroke-dasharray: 29 299;
  }
  .hamBurgerIc path:nth-child(3) {
    transform-origin: 35% 63%;
  }
  .hamBurgerIc path:nth-child(4) {
    stroke-dasharray: 29 299;
  }
  .hamBurgerIc path:nth-child(5) {
    transform-origin: 61% 52%;
  }
  .hamBurgerIc path:nth-child(6) {
    transform-origin: 62% 52%;
  }
  .hamBurgerIc.activeM path:nth-child(1) {
    transform: translateX(9px) translateY(1px) rotate(45deg);
  }
  .hamBurgerIc.activeM path:nth-child(2) {
    stroke-dasharray: 225 299;
    stroke-dashoffset: -72px;
  }
  .hamBurgerIc.activeM path:nth-child(3) {
    transform: translateX(9px) translateY(1px) rotate(-45deg);
  }
  .hamBurgerIc.activeM path:nth-child(4) {
    stroke-dasharray: 225 299;
    stroke-dashoffset: -72px;
  }
  .hamBurgerIc.activeM path:nth-child(5) {
    transform: translateX(9px) translateY(1px) rotate(-45deg);
  }
  .hamBurgerIc.activeM path:nth-child(6) {
    transform: translateX(9px) translateY(1px) rotate(45deg);
  }
  

