    margin: 0;
    border: 0;
    background-color: transparent;
}
.search-box button {
    position: absolute;
    right: 0;
    top: 0;  
    display: block;   
    cursor: pointer;
    width: 34px;
    height: 34px;
  }
#s-circle {
    position: relative;
    top: -8px;
    left: 0;
    width: 18px;
    height: 18px;
    margin-top: 0;
    border-width: 1px;
    border: 2px solid #000;
    background-color: transparent;
    border-radius: 50%;
    transition: 0.3s ease all;
  }
  
  .search-box button span {
    position: absolute;
    top: 18px;
    left: 12px;
    display: block;
    width: 10px;
    height: 2px;
    background-color: transparent;
    border-radius: 10px;
    transform: rotateZ(52deg);
    transition: 0.3s ease all;
  }
  
  .search-box button span:before,
  .search-box button span:after {
    content: "";
    position: absolute;
    bottom: 0;
    right: 0;
    width: 10px;
    height: 2px;
    background-color: #000;
    border-radius: 10px;
    transform: rotateZ(0);
    transition: 0.3s ease all;
  }
  
  #s-cover:hover #s-circle {
    top: 0px;
    width: 20px;
    height: 2px;
    border-width: 0;
    background-color: #000;
    border-radius: 20px;
  }
  
  #s-cover:hover span {
    top: 50%;
    left: 0;
    width: 25px;
    margin-top: -9px;
    transform: rotateZ(0);
  }
  
  #s-cover:hover button span:before {
    bottom: -4px;
    transform: rotateZ(52deg);
  }
  
  #s-cover:hover button span:after {
    bottom: -12px;
    transform: rotateZ(-52deg);
  }
  #s-cover:hover button span:before,
  #s-cover:hover button span:after {
    right: 1px;
    width: 10px;
    background-color: #000;
  }
