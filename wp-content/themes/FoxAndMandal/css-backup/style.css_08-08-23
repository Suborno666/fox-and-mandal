  .case-swiper-container {
    padding: 120px 0 !important;
    width: 100%;
    height: 45vw;
    transition: opacity 0.6s ease;
    margin-bottom:160px;
  }
  .case-swiper-container.swiper-container-coverflow {
    padding: 1% 0;
  }
  .case-swiper-container.loading {
    opacity: 0;
    visibility: hidden;
  }
  .case-swiper-container:hover .swiper-button-prev,
  .case-swiper-container:hover .swiper-button-next {
    transform: translateX(0);
    opacity: 1;
    visibility: visible;
  }
  
  .case-swiper-container .swiper-slide {
    background-position: center;
    background-size: cover;
  }
  .case-swiper-container .swiper-slide .entity-img {
    display: none;
  }
