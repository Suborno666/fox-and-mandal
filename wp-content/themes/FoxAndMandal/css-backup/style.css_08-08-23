    margin-bottom: 30px;
  }
  .case-swiper-container .swiper-slide .content .title a{
    color: #fff;
  }
  .case-swiper-container .swiper-pagination{
    display: none;
  }
  .case-swiper-container .swiper-slide .content .caption {
    display: block;
    font-size: 16px;
    line-height: 1.4;
  }
  .case-swiper-container .swiper-button-next, .case-swiper-container .swiper-button-prev{
    top: 65%;
  }
  [class^=swiper-button-] {
    width: 44px;
    opacity: 0;
    visibility: hidden;
  }
  
  .case-swiper-container .swiper-button-prev {
    transform: translateX(50px);
  }
  
  .case-swiper-container .swiper-button-next {
    transform: translateX(-50px);
  }
  
  .swiper-container-horizontal > .swiper-pagination-bullets .swiper-pagination-bullet {
    margin: 0 9px;
    position: relative;
    width: 12px;
    height: 12px;
    background-color: #fff;
