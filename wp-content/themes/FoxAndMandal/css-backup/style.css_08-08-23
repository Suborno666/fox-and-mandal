  .innerBox{    
    padding: 50px;
    position: relative;
    z-index: 2;
  }
  .serviceBox{
    position: relative;
    overflow: hidden;
  }
  .serviceBox::before{
    width: 100%;
    height: 100%;
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;    
    content: "";
    border: 1px solid rgba(10, 29, 53, 0.1);
    left: 0;
    z-index:1;
    transition: 0.5s all;
    opacity: 1;
    visibility: visible;
    margin: 0 auto;
    transform: scale(1);
  }
  .serviceBox::after{
    width: 100%;
    height: 94%;
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color:rgb(0 0 0 / 83%);    
    content: "";
    border: 1px solid rgba(119, 120, 121, 0.1);
    left: 0;
    z-index:1;
    transition: 0.5s all;
    visibility: hidden;
    margin: 0 auto;
    opacity: 0;
    visibility: hidden;
    transform: scale(0);
  }
  .serviceBox:hover:before{
    opacity: 0;
    visibility: hidden;
    transform: scale(1);
  }
  .serviceBox:hover:after{
    opacity: 1;
    visibility: visible;
    transform: scale(0.93);
  }
  .pbmit-service-style-4 .serviceBox:hover .pbmit-service-title a, .pbmit-service-style-4 .serviceBox:hover .pbmit-service-content p{
    color: #fff;
  }
